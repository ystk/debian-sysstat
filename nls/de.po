#, c-format
msgid "Usage: %s [ options ] [ <interval> [ <count> ] ]\n"
msgstr "Aufruf: %s [ optionen... ] [ <intervall> [ <anzahl> ] ]\n"

#: iostat.c:84
#, c-format
msgid ""
"Options are:\n"
"[ -c ] [ -d ] [ -N ] [ -n ] [ -h ] [ -k | -m ] [ -t ] [ -V ] [ -x ] [ -z ]\n"
"[ <device> [...] | ALL ] [ -p [ <device> [,...] | ALL ] ]\n"
