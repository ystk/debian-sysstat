#, c-format
msgid "System activity data file: %s (%#x)\n"
msgstr "Systemaktivitätendatei: %s (%#x)\n"

#: sadf.c:618
#, c-format
msgid "Host: "
msgstr "Rechner: "

#: sadf.c:624
#, c-format
